{
  "name": "meteo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve && npm run watch-css",
    "build": "vue-cli-service build",
    "sass-dev": "sass --watch --update --style=expanded src/scss:src/scss",
    "sass-prod": "sass --no-source-map --style=compressed src/scss:src/scss",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/"
  },
  "dependencies": {
    "@fawmi/vue-google-maps": "^0.9.79",
    "@googlemaps/js-api-loader": "^1.15.1",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-awesome-swiper": "^5.0.1",
    "vue-carousel": "^0.18.0",
    "vue-google-maps": "^0.1.21",
    "vue-slick-carousel": "^1.0.6",
    "vue2-google-maps": "^0.10.7",
    "vue2-leaflet": "^2.7.1"
  },
  "devDependencies": {
    "@vue-leaflet/vue-leaflet": "^0.9.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "leaflet": "^1.9.3",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "vue-svg-loader": "^0.16.0",
    "webpack": "^5.82.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "description": "## Project setup ``` npm install ```",
  "main": "n",
  "author": "",
  "license": "ISC"
}
